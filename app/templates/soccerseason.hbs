{{outlet}}

<h1>League</h1>

<h2>{{ model.caption }}</h2>

<h1>Teams:</h1>
<ul>
  {{#each model.teams as |team|}}
    <li>{{team.name}}</li>
  {{/each}}
</ul>

<h1>League Table</h1>
<ul>
  {{#each model.leagueTable.standing as |standing|}}
    <li>
      <img src={{standing.crestURI}} height=20 width=20>
      <span>{{standing.rank}} - {{standing.team}} - {{standing.points}}</span>
    </li>
  {{/each}}
</ul>
